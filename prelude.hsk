
{{ A void type has no constructors }}
-- This is a comment line

decltype 'void 

decltype 'unit = unit

decltype 'bool 
	= true {{ this is true }}
	| false {{ this is false }}

decltype 'either a b 
	= left a {{ it's either the left-hand side }}
	| right b {{ or the right-hand side }}

decltype 'prod a b
	= a * b

decltype 'n 
	= zero 
	| succ 'n
